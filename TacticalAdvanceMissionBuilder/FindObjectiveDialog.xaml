<Window x:Class="TacticalAdvanceMissionBuilder.FindObjectiveDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FindObjectiveDialog" Height="140.977" Width="434.211" Loaded="Window_Loaded">
    <Grid>
        <Label Content="Enter an objective ID" HorizontalAlignment="Left" Margin="10" VerticalAlignment="Top" FontWeight="Bold"/>
        <TextBox x:Name="IdTextBox" HorizontalAlignment="Left" Height="25" Margin="10,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="383"/>
        <Button x:Name="CancelButton"  Content="Cancel" HorizontalAlignment="Left" Margin="261,79,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" Click="CancelButton_Click"/>
        <Button x:Name="FindButton" Content="Find" HorizontalAlignment="Left" Margin="341,79,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="FindButton_Click"/>

    </Grid>
</Window>
